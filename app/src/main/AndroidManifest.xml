<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" >

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme._420_14D_TP1_FreDig"
        tools:targetApi="31" >
        <activity
            android:name=".AcceuilActivity"
            android:exported="false"
            android:label="@string/title_activity_acceuil"
            android:theme="@style/Theme._420_14D_TP1_FreDig"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".RouletteActivity"
            android:exported="false"
            android:label="@string/title_activity_roulette"
            android:theme="@style/Theme._420_14D_TP1_FreDig"
            android:parentActivityName=".AcceuilActivity"/>
        <activity
            android:name=".BanqueActivity"
            android:exported="false"
            android:label="@string/title_activity_banque"
            android:theme="@style/Theme._420_14D_TP1_FreDig"
            android:parentActivityName=".AcceuilActivity"/>
        <activity
            android:name=".MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>